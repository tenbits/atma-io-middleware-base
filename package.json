{
  "name": "atma-io-middleware-base",
  "description": "Atma IO middleware abstraction",
  "version": "1.0.0",
  "homepage": "https://github.com/tenbits/atma-io-middleware-base",
  "contributors": [],
  "main": "./lib/index.js",
  "author": "Alexander Kit <alex.kit@atmajs.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenbits/atma-io-middleware-base.git"
  },
  "bugs": {
    "url": "https://github.com/tenbits/atma-io-middleware-base/issues"
  },
  "licenses": [
    "MIT"
  ],
  "devDependencies": {
    "app-bundler": "0.0.22",
    "atma-loader-babel": "^1.1.30",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "build": "app-bundler"
  },
  "app-bundler": {
    "file": "src/exports.js",
    "outputMain": "lib/index.js",
    "package": {
      "module": "commonjs",
      "commonjs": {
        "output": "simplified"
      }
    },
    "dynamicDependencies": [
      "appcfg",
      "atma-utils",
      "atma-io",
      "events"
    ],
    "middlewares": {
      "js": [
        "importer:read",
        "atma-loader-babel:read"
      ]
    }
  },
  "keywords": [],
  "atma": {
    "settings": {
      "atma-loader-babel": {
        "babel": {
          "presets": [
            "es2015"
          ]
        }
      }
    }
  },
  "dependencies": {
    "appcfg": "^0.2.44",
    "atma-utils": "^0.1.8"
  }
}
